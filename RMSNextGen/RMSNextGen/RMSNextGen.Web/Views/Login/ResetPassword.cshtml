@{
    Layout = "_LoginLayout";
}
<div>
    <h2>Welcome KrishnaVeni</h2>
    <p>Please Provide the new password Below</p>
    

    <div>
        <label>New Password:</label>
        <input type="text" id="txtEmail" name="txtEmail" class="input-control" />
    </div>
    <div>
        <label>Confirm Password:</label>
        <input type="password" id="txtPassword" name="txtPassword" class="input-control" />
    </div>
    <div>
        <button type="button" class="btn btn-primary">Reset Password</button>
        <button type="button" class="btn btn-secondary">Back to Login</button>
        
        
    </div>
    
</div>